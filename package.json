{
  "name": "pesel-tools",
  "version": "0.15.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "start:ssr": "node dist/pesel-tools/server/server.mjs",
    "serve:ssr:pesel-tools": "node dist/pesel-tools/server/server.mjs",
    "lint": "ng lint",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:scss:fix": "stylelint \"src/**/*.scss\" --fix",
    "release": "commit-and-tag-version",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.5",
    "@angular/common": "^21.1.5",
    "@angular/compiler": "^21.1.5",
    "@angular/core": "^21.1.5",
    "@angular/forms": "^21.1.5",
    "@angular/platform-browser": "^21.1.5",
    "@angular/platform-browser-dynamic": "^21.1.5",
    "@angular/platform-server": "^21.1.5",
    "@angular/router": "^21.1.5",
    "@angular/service-worker": "^21.1.5",
    "@angular/ssr": "^21.2.0",
    "@eslint/js": "^9.32.0",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.2.0",
    "@angular/cli": "^21.2.0",
    "@angular/compiler-cli": "^21.1.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@cypress/schematic": "^3.0.0",
    "@testing-library/angular": "^19.1.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "@vitest/ui": "^4.0.12",
    "angular-eslint": "21.2.0",
    "commit-and-tag-version": "^12.6.1",
    "commitlint": "^19.8.1",
    "cypress": "latest",
    "eslint": "^9.28.0",
    "husky": "^8.0.0",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.1.2",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "stylelint": "^16.22.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-scss": "^6.12.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.33.1",
    "vite": "^7.2.6",
    "vitest": "^4.0.8"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix"
    ],
    "*.{scss,css}": [
      "stylelint --fix"
    ],
    "*.{ts,js,scss,json,html,md}": [
      "prettier --write"
    ]
  },
  "type": "module"
}
